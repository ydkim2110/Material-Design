<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anti2110.materialdesign">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".toolbar.ToolbarActivity" />
        <activity android:name=".navigationDraw.NavigationDrawActivity" />
        <activity android:name=".testField.TextFieldActivity" />
        <activity
            android:name=".collapsingToolbar.CollapsingToolbarActivity"
            android:label="New York City" />
        <activity android:name=".swipeRefreshLayout.SwipeRefreshActivity" />
        <activity android:name=".pickers.TimePickerActivity" />
        <activity android:name=".pickers.DatePickerActivity" />
        <activity android:name=".viewPager_ImageSlide.ViewPagerImageSlideActivity"></activity>
    </application>

</manifest>